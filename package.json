  "scripts": {
    "start": "nx serve web",
    "start:web": "expo start --web",
    "start:desktop": "concurrently \"expo start --web\" \"nx serve desktop\"",
    "start:mobile": "expo start",
    "start:api": "nx serve api",
    "build": "nx build web",
