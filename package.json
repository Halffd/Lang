    "build:mobile": "expo build",
    "build:api": "nx build api",
    "build:all": "nx run-many --target=build --projects=web,desktop,mobile,api --parallel",
    "test": "nx test",
    "lint": "nx lint",
    "dev": "concurrently \"npm run start:web\" \"npm run start:api\"",
    "dev:desktop": "concurrently \"npm run start:web\" \"npm run start:desktop\" \"npm run start:api\""
